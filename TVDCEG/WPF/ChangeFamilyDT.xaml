<Window x:Class="TVDCEG.WPF.ChangeFamilyDT"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TVDCEG.WPF"
             mc:Ignorable="d" 
             Title="Iven EXT: Add prefix sheet" WindowStartupLocation="CenterScreen" Width="300" Height="200" ResizeMode="NoResize"
             d:DesignHeight="250" d:DesignWidth="500">
    <Window.Resources>
        <ResourceDictionary Source="..//Style/Style.xaml"></ResourceDictionary>
    </Window.Resources>
    <DockPanel>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <ComboBox Name="Type_dt" Width="200" Grid.Row="0" HorizontalAlignment="Left" Style="{StaticResource ComboBoxStyle}"></ComboBox>
            <Button x:Name="Btn_ok" Content="OK" Grid.Row="3" Style="{StaticResource ButtonStyle}" Click="Btn_ok_Click"></Button>
        </Grid>
    </DockPanel>
</Window>
