<Window x:Class="TVDCEG.FrmDimgrid"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TVDCEG"
        mc:Ignorable="d"
        Loaded="Window_Loaded" WindowStartupLocation="CenterScreen"
        Title="Iven EXT: Dimension grids" Height="200" Width="340">
    <Window.Resources>
        <ResourceDictionary Source="Style/Style.xaml"></ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <ComboBox Name="Cbb_typedim" Grid.Row="0" Grid.Column="1" Height="25" Width="200" Style="{StaticResource ComboBoxStyle}"></ComboBox>
        <Button x:Name="btn_Ok" Content="OK" Grid.Row="3" Grid.Column="1" Height="30" Width="150" Style="{StaticResource ButtonStyle}" Click="btn_Ok_Click"></Button>
        <Label Content="Type Dimension:" VerticalAlignment="Center"></Label>
    </Grid>
</Window>
