<Window x:Class="TVDCEG.WPF.FrmRenamesheet"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TVDCEG.WPF"
             mc:Ignorable="d" 
             Title="Iven EXT: Rename Sheet" WindowStartupLocation="CenterScreen"
             Width="400" Height="500">
    <Window.Resources>
        <ResourceDictionary Source="..//Style/Style.xaml"></ResourceDictionary>
    </Window.Resources>
    <Grid Margin="0,0,2,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250"></ColumnDefinition>
            <ColumnDefinition Width="150"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="35"></RowDefinition>
            <RowDefinition Height="35"></RowDefinition>
        </Grid.RowDefinitions>
        <Button x:Name="Btn_Ok" Content="OK" Grid.Row="2" Grid.Column="1" Style="{StaticResource ButtonStyle}" Height="Auto" Margin="3,4,0,0" HorizontalAlignment="Left" Width="119"/>
        <ListBox x:Name="lb" Margin="10,0,0,0" />
    </Grid>
</Window>
